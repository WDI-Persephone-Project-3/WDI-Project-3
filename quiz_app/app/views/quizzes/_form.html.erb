<%= form_for @quiz, html: {method: "get"} do %>
  <%= label_tag(:test_day, "Date:") %>
  <%= date_field_tag(:test_day) %>
  <br>
  <%= label_tag(:question, "Question:") %>
  <%= text_field_tag(:question) %> <!-- HELP: How do I get other options in here without having to include them in here itself -->
  <br>
  <%= label_tag(:answer, "Answer") %>
  <%= text_field_tag(:answer) %>
  <br>
  <%= submit_tag("Create") %>
<% end %>